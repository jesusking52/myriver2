<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingAnimationLayout">
        <attr name="size">
            <enum name="large" value="0"/>
            <enum name="small" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TagGroupView">
        <attr name="rowPaddingTop" format="dimension"/>
        <attr name="rowPaddingBottom" format="dimension"/>
        <attr name="rowGravity">
            <flag name="center_vertical" value="0x10"/>
            <flag name="center_horizontal" value="0x01"/>
            <flag name="center" value="0x11"/>
        </attr>
    </declare-styleable>
</resources>